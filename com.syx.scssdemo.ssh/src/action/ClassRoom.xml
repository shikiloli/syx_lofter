<?xml version="1.0" encoding="UTF-8" ?>

<!DOCTYPE struts PUBLIC
	"-//Apache Software Foundation//DTD Struts Configuration 2.5//EN"
	"http://struts.apache.org/dtds/struts-2.5.dtd">

<!-- START SNIPPET: xworkSample -->
<struts>  
    <package name="classRoom" extends="default" namespace="/classRoom">
    	<action name="*">
    	<result>/WEB-INF/content/admin/{1}.jsp</result>
    	</action>
    	<action name="ClassRoomShow" class="action.ClassRoomAction" method="ClassRoomShow">
    		<result name="success">/WEB-INF/content/admin/ClassRoomMgr.jsp</result>
    	</action>
    	<action name="ClassRoomInsert" class="action.ClassRoomAction" method="ClassRoomInsert">
    		<result name="success">/WEB-INF/content/admin/ClassRoomMgr.jsp</result>
    	</action>
    	<action name="ClassRoomDelete" class="action.ClassRoomAction" method="ClassRoomDelete">
    		<result name="success">/WEB-INF/content/admin/ClassRoomMgr.jsp</result>
    	</action>
    	<action name="ClassRoomUpdate" class="action.ClassRoomAction" method="ClassRoomUpdate">
    		<result name="success">/WEB-INF/content/admin/ClassRoomMgr.jsp</result>
    	</action>
    </package>
    
 </struts>



